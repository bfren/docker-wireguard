#!/command/with-contenv bash

set -euo pipefail
export BF_E=`basename ${0}`


#======================================================================================================================
# Generate WireGuard configuration.
#======================================================================================================================

export PRIVATE_KEY=`cat ${WIREGUARD_PRIVATE_KEY_FILE}`
esh \
    -o ${WIREGUARD_CONF} \
    -s /bin/bash \
    ${BF_TEMPLATES}/interface.conf.esh
bf-done
